<%= form_for @upload, html: { multipart: true } do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :attachment %>
      <%= f.file_field :attachment %>
      <%= f.label :folder_id %>
      <%= f.select :folder_id, @current_user.folders.pluck(:name, :id) %>     

      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>